{
  "restructuredtext.confPath": "",
  "python.formatting.provider": "black",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": {
      "when": "$(basename).py"
    },
    "**/__pycache__": true
  },
  "python.pythonPath": "/usr/bin/python"
}
